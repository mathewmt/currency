  body
    .container_12
      .section.content
        .wrapper
          .note-tabpanel
            .note-tabs(style='float: left; clear: none; ')
              ul
                li.active Obverse
                li Reverse
            div(style='clear:both')
            .note-tabdetails
              #box1.note-box
                .note500
                img(src='http://localhost:5000/uploads/#{side1}', alt='RBI 500')
                .noteho500
                img(src='http://localhost:5000/uploads/#{side1} style="display:none;"', alt='RBI 500')
                
                -for (var i = 0; i < side1features.length; i++) {
                .notewrap
                    
                    div(class="numberbar numberbar"+i+1)
                      .numcircle #{i}
                      div(class="numline numline"+i+1)
                      .imgbar.descimg1
                        img(src='http://localhost:5000/uploads/#{side1features[i].image}', alt='RBI')
                    .descbar(style='display: none;')
                    .showdescbar
                        .descnum #{k}
                        .desctxt #{side1features[i].feature}
                -}
              #box2.note-box
                .note500
                img(src='http://localhost:5000/uploads/#{side2}', alt='RBI 500')
                .noteho500
                img(src='http://localhost:5000/uploads/#{side2}', alt='RBI 500')
                -for (var j = 0; j <side2features.length; j++) {
                  .notewrap
                      .numberbar.numberbar #{i+j}
                        .numcircle #{i+j}
                        .numline.numline{i+j+1}
                        .imgbar.descimg{i+j+1}
                          img(src='http://localhost:5000/uploads/#{side2features[j].image}', alt='RBI')
                      .descbar
                      .showdescbar
                          .descnum #{i+j+1}
                          .desctxt #{side2features[j].feature}
                -}
                  
    .clearfix
